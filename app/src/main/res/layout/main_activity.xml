<?xml version="1.0" encoding="utf-8"?>
<LinearLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical"
    >



    <com.example.materialdesign.ui.main.EquilateralImageView
        android:id="@+id/image_view"
        android:layout_width="200dp"
        android:layout_height="300dp"
        android:layout_gravity="center"
        android:contentDescription="@string/photo_of_the_day_stub"
        app:srcCompat="@drawable/ic_no_photo_vector" />


    <Button
        style="@style/MarsMainButton"
        android:id="@+id/outlinedButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Outlined button MarsMainButton"
        app:icon="@drawable/ic_baseline_add_24"

        />

    <Button
        android:id="@+id/outlinedButtonWarning"
        style="@style/MarsMainButton.Warning"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Outlined button outlinedButtonWarning"
        app:icon="@drawable/ic_baseline_add_24"

        />

    <Button
        android:id="@+id/outlinedButtonApp"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Outlined button outlinedButtonApp"
        app:icon="@drawable/ic_baseline_add_24"

        />

    <Button
        android:id="@+id/outlinedButtonMars"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Outlined button outlinedButtonApp"
        app:icon="@drawable/ic_baseline_add_24"

        />

    <Button
        android:id="@+id/Go_TO_bottom_navigation_view"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go_TO_bottom_navigation_view"
        app:icon="@drawable/ic_baseline_add_24"

        />
</LinearLayout>



